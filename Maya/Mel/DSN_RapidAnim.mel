global proc rightToLeftControls()
{
	string $rigName[] = `ls -sl`;

	//ARMS////////////////////////////////////////////////////////////////////////
	$rClavicleC_R = `xform -q -ro {$rigName[0] + "_rClavicleC"}`;
	setAttr ({$rigName[0] + "_lClavicleC.rotate"}, $rClavicleC_R[0], $rClavicleC_R[1], $rClavicleC_R[2]);
	
	//FK SHOULDERS
	$rShoulderFKC_R = `xform -q -ro {$rigName[0] + "_rShoulderFKC"}`;
	$rShoulderFKC_SX = `getAttr {$rigName[0] + "_rShoulderFKC.sx"}`;	
	$rShoulderFKC_Orient = `getAttr {$rigName[0] + "_rShoulderFKC.ShoulderOrient"}`;	
	setAttr ({$rigName[0] + "_lShoulderFKC.rotate"}, $rShoulderFKC_R[0], $rShoulderFKC_R[1], $rShoulderFKC_R[2]);	
	setAttr {$rigName[0] + "_lShoulderFKC.sx"} $rShoulderFKC_SX;
	setAttr {$rigName[0] + "_lShoulderFKC.ShoulderOrient"} $rShoulderFKC_Orient;
	
		if (`objExists ($rigName[0] + "_rShoulderFKC.ClavicleInfluence")`)
	{
		$rShoulderFKC_ClavInf = `getAttr {$rigName[0] + "_rShoulderFKC.ClavicleInfluence"}`;	
		setAttr {$rigName[0] + "_lShoulderFKC.ClavicleInfluence"} $rShoulderFKC_ClavInf;
	}
	
	//FK ELBOWS
	$rElbowFKC_R = `getAttr {$rigName[0] + "_rElbowFKC.ry"}`;
	setAttr {$rigName[0] + "_lElbowFKC.ry"} $rElbowFKC_R;
	//FK WRISTS
	$rWristFKC_R = `xform -q -ro {$rigName[0] + "_rWristFKC"}`;
	$rWristFKC_SX = `getAttr {$rigName[0] + "_rWristFKC.sx"}`;	
	setAttr ({$rigName[0] + "_lWristFKC.rotate"}, $rWristFKC_R[0], $rWristFKC_R[1], $rWristFKC_R[2]);	
	setAttr {$rigName[0] + "_lWristFKC.sx"} $rWristFKC_SX;
	
	//IK ELBOWS
	$rElbowIKC_T = `xform -q -t {$rigName[0] + "_rElbowIKC"}`;
	setAttr ({$rigName[0] + "_lElbowIKC.translate"}, ($rElbowIKC_T[0] * -1), $rElbowIKC_T[1], $rElbowIKC_T[2]);

	//IK WRISTS
	$rWristIKC_R = `xform -q -ro {$rigName[0] + "_rWristIKC"}`;
	$rWristIKC_T = `xform -q -t {$rigName[0] + "_rWristIKC"}`;
	$rWristIKC_SX = `getAttr {$rigName[0] + "_rWristIKC.sx"}`;
	$rWristIKC_Stretch = `getAttr {$rigName[0] + "_rWristIKC.Stretch"}`;	
	$rWristIKC_ElbowLock = `getAttr {$rigName[0] + "_rWristIKC.ElbowLock"}`;
	$rWristIKC_Hips = `getAttr {$rigName[0] + "_rWristIKC.Hips"}`;	
	$rWristIKC_Chest = `getAttr {$rigName[0] + "_rWristIKC.Chest"}`;	
	$rWristIKC_Head = `getAttr {$rigName[0] + "_rWristIKC.Head"}`;
	
	setAttr ({$rigName[0] + "_lWristIKC.translate"}, ($rWristIKC_T[0] * -1), ($rWristIKC_T[1] * -1), ($rWristIKC_T[2] * -1));
	setAttr ({$rigName[0] + "_lWristIKC.rotate"}, $rWristIKC_R[0], $rWristIKC_R[1], $rWristIKC_R[2]);	
	setAttr {$rigName[0] + "_lWristIKC.sx"} $rWristIKC_SX;
	setAttr {$rigName[0] + "_lWristIKC.Stretch"} $rWristIKC_Stretch;
	setAttr {$rigName[0] + "_lWristIKC.ElbowLock"} $rWristIKC_ElbowLock;
	setAttr {$rigName[0] + "_lWristIKC.Hips"} $rWristIKC_Hips;
	setAttr {$rigName[0] + "_lWristIKC.Chest"} $rWristIKC_Chest;
	setAttr {$rigName[0] + "_lWristIKC.Head"} $rWristIKC_Head;

	//PALM
	$rPalmC_R = `xform -q -ro {$rigName[0] + "_rPalmC"}`;
	$rPalmC_R = `xform -q -ro {$rigName[0] + "_rPalmC"}`;
	setAttr ({$rigName[0] + "_lPalmC.rotate"}, $rPalmC_R[0], $rPalmC_R[1], $rPalmC_R[2]);
	setAttr ({$rigName[0] + "_lPalmC.rotate"}, $rPalmC_R[0], $rPalmC_R[1], $rPalmC_R[2]);
		
	
	//ARM SWITCH
	$rArmSwitch = `getAttr {$rigName[0] + "_rArmSwitchC.SwitchIkFk"}`;
	$rArmVolume = `getAttr {$rigName[0] + "_rArmSwitchC.AutoVolume"}`;
	setAttr {$rigName[0] + "_lArmSwitchC.SwitchIkFk"} $rArmSwitch;
	setAttr {$rigName[0] + "_lArmSwitchC.AutoVolume"} $rArmVolume;
	
	//LEGS////////////////////////////////////////////////////////////////////////
	//FK HIPS
	$rHipFKC_R = `xform -q -ro {$rigName[0] + "_rHipFKC"}`;
	$rHipFKC_SX = `getAttr {$rigName[0] + "_rHipFKC.sx"}`;	
	$rHipFKC_Orient = `getAttr {$rigName[0] + "_rHipFKC.HipOrient"}`;	
	setAttr ({$rigName[0] + "_lHipFKC.rotate"}, $rHipFKC_R[0], $rHipFKC_R[1], $rHipFKC_R[2]);
	setAttr {$rigName[0] + "_lHipFKC.sx"} $rHipFKC_SX;
	setAttr {$rigName[0] + "_lHipFKC.HipOrient"} $rHipFKC_Orient;

	//FK KNEES
	$rKneeFKC_R = `getAttr {$rigName[0] + "_rKneeFKC.rz"}`;
	setAttr {$rigName[0] + "_lKneeFKC.rz"} $rKneeFKC_R;
	//FK ANKLES
	$rAnkleFKC_R = `xform -q -ro {$rigName[0] + "_rAnkleFKC"}`;
	setAttr ({$rigName[0] + "_lAnkleFKC.rotate"}, $rAnkleFKC_R[0], $rAnkleFKC_R[1], $rAnkleFKC_R[2]);	
	//FK BALLS
	$rBallFKC_R = `xform -q -ro {$rigName[0] + "_rBallFKC"}`;
	setAttr ({$rigName[0] + "_lBallFKC.rotate"}, $rBallFKC_R[0], $rBallFKC_R[1], $rBallFKC_R[2]);
	
	//IK KNEES
	$rKneeIKC_T = `xform -q -t {$rigName[0] + "_rKneeIKC"}`;
	setAttr ({$rigName[0] + "_lKneeIKC.translate"}, ($rKneeIKC_T[0] * -1), $rKneeIKC_T[1], $rKneeIKC_T[2]);

	//IK FEET
	$rFootIKC_R = `xform -q -ro {$rigName[0] + "_rFootIKC"}`;
	$rFootIKC_T = `xform -q -t {$rigName[0] + "_rFootIKC"}`;
	$rFootIKC_Stretch = `getAttr {$rigName[0] + "_rFootIKC.Stretch"}`;	
	$rFootIKC_KneeLock = `getAttr {$rigName[0] + "_rFootIKC.KneeLock"}`;
	$rFootIKC_footTilt = `getAttr {$rigName[0] + "_rFootIKC.footTilt"}`;	
	$rFootIKC_heelBall = `getAttr {$rigName[0] + "_rFootIKC.heelBall"}`;	
	$rFootIKC_toeUpDn = `getAttr {$rigName[0] + "_rFootIKC.toeUpDn"}`;	
	$rFootIKC_ballSwivel = `getAttr {$rigName[0] + "_rFootIKC.ballSwivel"}`;
	
	setAttr {$rigName[0] + "_lFootIKC.tx"} ($rFootIKC_T[0] * -1);
	setAttr {$rigName[0] + "_lFootIKC.ty"} ($rFootIKC_T[1] * -1);
	setAttr {$rigName[0] + "_lFootIKC.tz"} ($rFootIKC_T[2] * -1);
	setAttr {$rigName[0] + "_lFootIKC.rx"} $rFootIKC_R[0];
	setAttr {$rigName[0] + "_lFootIKC.ry"} $rFootIKC_R[1];
	setAttr {$rigName[0] + "_lFootIKC.rz"} $rFootIKC_R[2];	
	setAttr {$rigName[0] + "_lFootIKC.Stretch"} $rFootIKC_Stretch;
	setAttr {$rigName[0] + "_lFootIKC.KneeLock"} $rFootIKC_KneeLock;
	setAttr {$rigName[0] + "_lFootIKC.footTilt"} $rFootIKC_footTilt;
	setAttr {$rigName[0] + "_lFootIKC.heelBall"} $rFootIKC_heelBall;
	setAttr {$rigName[0] + "_lFootIKC.toeUpDn"} $rFootIKC_toeUpDn;
	setAttr {$rigName[0] + "_lFootIKC.ballSwivel"} $rFootIKC_ballSwivel;
	
	//IK TOE
	$rToeIKC_R = `xform -q -ro {$rigName[0] + "_rToeIKC"}`;
	setAttr ({$rigName[0] + "_lToeIKC.rotate"}, $rToeIKC_R[0], $rToeIKC_R[1], $rToeIKC_R[2]);
	
	//LEG SWITCH
	$rLegSwitch = `getAttr {$rigName[0] + "_rLegSwitchC.SwitchIkFk"}`;
	$rLegVolume = `getAttr {$rigName[0] + "_rLegSwitchC.AutoVolume"}`;
	setAttr {$rigName[0] + "_lLegSwitchC.SwitchIkFk"} $rLegSwitch;
	setAttr {$rigName[0] + "_lLegSwitchC.AutoVolume"} $rLegVolume;
	
	if (`objExists ($rigName[0] + "_rElbowCurveC")`)
		{
			rightToLeftControlsToonArms;
		}
	if (`objExists ($rigName[0] + "_rKneeCurveC")`)
		{
			rightToLeftControlsToonLegs;
		}
		
	rightToLeftControlsFingers;
}

global proc rightToLeftControlsCheck()
{
	string $rig[] = `ls -sl`;
	int $rigSize = `size $rig`;
	if ($rigSize > 0)
	{
		int $identification = `getAttr {$rig[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}
	
		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification == 1122)
		{rightToLeftControls;}
		else
		{warning "YOU MUST SELECT A CONTROLLER OF THE CHARACTER YOU WISH TO MIRROR\n";}
	}
	else
	{warning "YOU MUST SELECT A CONTROLLER OF THE CHARACTER YOU WISH TO MIRROR\n";}

select $rig;	
}



global proc mirrorToonArms()
{
	string $rigName[] = `ls -sl`;
	//CURVE 
	$lArmCurve = `getAttr {$rigName[0] + "_lArmSwitchC.curve"}`;
	$rArmCurve = `getAttr {$rigName[0] + "_rArmSwitchC.curve"}`;	
	setAttr {$rigName[0] + "_lArmSwitchC.curve"} $rArmCurve;
	setAttr {$rigName[0] + "_rArmSwitchC.curve"} $lArmCurve;
	
	//MACRO CONTROLS
	$lUpperArmCurveC_R = `xform -q -ro {$rigName[0] + "_lUpperArmCurveC"}`;
	$lUpperArmCurveC_T = `xform -q -t {$rigName[0] + "_lUpperArmCurveC"}`;
