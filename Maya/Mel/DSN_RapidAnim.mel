		$rThumbJ1C_R = `xform -q -ro {$rigName[0] + "_rThumbJ1C"}`;
		$rThumbJ1C_S = `xform -q -r -s {$rigName[0] + "_rThumbJ1C"}`;
		setAttr ({$rigName[0] + "_lThumbJ1C.rotate"}, $rThumbJ1C_R[0], $rThumbJ1C_R[1], $rThumbJ1C_R[2]);
