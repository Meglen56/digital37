		setAttr {$rigName[0] + "_rHipFKC.rotateX"} $rHipFKOrientLctr[0];
		setAttr {$rigName[0] + "_rHipFKC.rotateY"} $rHipFKOrientLctr[1];
		setAttr {$rigName[0] + "_rHipFKC.rotateZ"} $rHipFKOrientLctr[2];
		
		$rKneeIK_R = `getAttr {$rigName[0] + "_rKneeJIK.rz"}`;
		setAttr {$rigName[0] + "_rKneeFKC.rotateZ"} ($rKneeIK_R * 1);
		
		$rAnkleIKLctr_R = `xform -q -ro {$rigName[0] + "_rAnkleIKLctr"}`;
		setAttr {$rigName[0] + "_rAnkleFKC.rotateX"} ($rAnkleIKLctr_R[1] * -1);
		setAttr {$rigName[0] + "_rAnkleFKC.rotateY"} ($rAnkleIKLctr_R[2] * 1);
		setAttr {$rigName[0] + "_rAnkleFKC.rotateZ"} ($rAnkleIKLctr_R[0] * -1);

		setAttr {$rigName[0] + "_rLegSwitchC.SwitchIkFk"} 1;

		$rHipIKScale = `getAttr {$rigName[0] + "_rHipJIK.sx"}`;
		setAttr {$rigName[0] + "_rHipFKC.sx"} $rHipIKScale;
	}
}

global proc selectAllControlsCheck()
{
	string $rig[] = `ls -sl`;
	int $rigSize = `size $rig`;
	if ($rigSize > 0)
	{
		int $identification = `getAttr {$rig[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}
	
		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification == 1122)
		{selectAllControls;}
		else
		{warning "YOU MUST SELECT A CONTROLLER OF THE CHARACTER YOU WISH TO MIRROR\n";}
	}
	else
	{warning "YOU MUST SELECT A CONTROLLER OF THE CHARACTER YOU WISH TO MIRROR\n";}
}

global proc selectAllControls()
{
		string $rigName[] = `ls -sl`;

	select {$rigName[0] + "_ROOTC"} {$rigName[0] + "_MainHipC"} {$rigName[0] + "_Spine??FKC"} {$rigName[0] + "_SpineMidIKC"}
	{$rigName[0] + "_SpineTopIKC"};
	if (`objExists ($rigName[0] + "_EyeAimC")`)
	{
		select -add {$rigName[0] + "_EyeAimC"};
	}
	if (`objExists ($rigName[0] + "_lEyeAimC")`)
	{
		select -add {$rigName[0] + "_lEyeAimC"};
	}
	if (`objExists ($rigName[0] + "_rEyeAimC")`)
	{
		select -add {$rigName[0] + "_rEyeAimC"};
	}
	if (`objExists ($rigName[0] + "_JawC")`)
	{
		select -add {$rigName[0] + "_JawC"};
	}
	select -add {$rigName[0] + "_HeadC"}
	{$rigName[0] + "_Neck01C"} {$rigName[0] + "_lLegSwitchC"} {$rigName[0] + "_lKneeIKC"} {$rigName[0] + "_lBallFKC"}
	{$rigName[0] + "_lAnkleFKC"} {$rigName[0] + "_lKneeFKC"} {$rigName[0] + "_lHipFKC"} {$rigName[0] + "_lElbowIKC"}
	{$rigName[0] + "_lElbowFKC"} {$rigName[0] + "_lShoulderFKC"} {$rigName[0] + "_lClavicleC"} {$rigName[0] + "_lToeIKC"}
	{$rigName[0] + "_rLegSwitchC"} {$rigName[0] + "_rKneeIKC"} {$rigName[0] + "_rBallFKC"} {$rigName[0] + "_rAnkleFKC"}
	{$rigName[0] + "_rKneeFKC"} {$rigName[0] + "_rHipFKC"} {$rigName[0] + "_rElbowIKC"} {$rigName[0] + "_rElbowFKC"}
	{$rigName[0] + "_rShoulderFKC"} {$rigName[0] + "_rClavicleC"} {$rigName[0] + "_rToeIKC"} {$rigName[0] + "_Neck02C"}
	{$rigName[0] + "_lPalmC"} {$rigName[0] + "_rPalmC"} {$rigName[0] + "_lWristIKC"} {$rigName[0] + "_lWristFKC"}
	{$rigName[0] + "_rWristIKC"} {$rigName[0] + "_rWristFKC"} {$rigName[0] + "_rArmSwitchC"} {$rigName[0] + "_lArmSwitchC"}
	{$rigName[0] + "_rFootIKC"} {$rigName[0] + "_MainC"} {$rigName[0] + "_lFootIKC"}
	{$rigName[0] + "_*Finger*J*C"};
	
	if (`objExists ($rigName[0] + "_lThumbJ1C")`)
	{
		select -add {$rigName[0] + "_*ThumbJ*C"};
	}
	
	if (`objExists ($rigName[0] + "_lToe01J1C")`)
	{
		select -add {$rigName[0] + "_*Toe*J*C"};
	}
	
	if (`objExists ($rigName[0] + "_lUpperArmCurveC")`)
	{
		select -add {$rigName[0] + "_*ArmCurveC"} {$rigName[0] + "_*ArmCurve*C"} {$rigName[0] + "_*ElbowCurveC"};
	}
	
	if (`objExists ($rigName[0] + "_lUpperLegCurveC")`)
	{
		select -add {$rigName[0] + "_*LegCurveC"} {$rigName[0] + "_*LegCurve*C"} {$rigName[0] + "_*KneeCurveC"};
	}
}


//UI
global proc CreateAnimUI()
{
global string $rigName;
string $form = `formLayout`;
columnLayout;

//FINGER SELECTIONS
frameLayout -collapsable true -label "Select Fingers & Toes"
	-width 204;

	rowColumnLayout -nc 2;
		button -label "Controls Up & Dn"
		-command "controlsUpDn";
		button -label "Controls Across"
		-command "controlsAcross";
		
		setParent ..;
		setParent ..;
		
//MIRROR POSE
frameLayout -collapsable true -label "Pose"
	-width 204;

	rowColumnLayout -nc 2;
		button -label "Right to Left"
		-command "rightToLeftControlsCheck";
		button -label "Left to Right"
		-command "leftToRightControlsCheck";
		button -label "Reset Transforms"
		-command "resetTransformsCheck";
		button -label "Reset Extras"
		-command "resetExtrasCheck";
		button -label "Mirror"
		-command "mirrorCheck";
		
		setParent ..;
		setParent ..;

		
//MATCH IK TO FK/FK TO IK
//4 CHECK BUTTONS AND 2 BUTTONS AT THE BOTTOM FOR "FK TO IK" AND "IK TO FK"
	frameLayout -collapsable true -label "IK/FK Match"
		-width 204;

    rowColumnLayout -nc 2;
    $leftArmMatch = `checkBox
		-label "Left Arm"
		-value 0
		leftArmMatch`;

    $rightArmMatch = `checkBox
		-label "Right Arm"
		-value 0
		rightArmMatch`;
	
    $leftLegMatch = `checkBox
		-label "Left Leg"
		-value 0
		leftLegMatch`;

    $rightLegMatch = `checkBox
		-label "Right Leg"
		-value 0
		rightLegMatch`;
		
		button -label "IK to FK"
		-command "matchIKtoFKCheck"
		matchIKtoFKCheck;		
		
		button -label "FK to IK"
		-command "matchFKtoIKCheck"
		matchFKtoIKCheck;
		
		setParent ..;
		setParent ..;  

//SELECT ALL CONTROLS
//CHECK BUTTONS FOR ARMS/LEGS *WISHLIST*
	frameLayout -collapsable true -label "Select Controls"
		-width 204;
	
		button -label "Select All"
		-command "selectAllControlsCheck"
		selectAllControlsCheck;
		
		setParent ..;
		setParent ..;
}

//PROCEDURE FOR WINDOW
global proc DSN_RapidAnim()                                        
{
	if (`window -exists DSN_BiPedAnimation`) 
	{
		deleteUI DSN_BiPedAnimation; 
	}
string $DSN_BiPedAnimation = `window
	-title "DSN_RapidAnim"
	-wh 204 180
	-rtf on
	-mnb false
	-mxb false
	-s 1
	DSN_BiPedAnimation`;

CreateAnimUI();

showWindow $DSN_BiPedAnimation;
window -e -w 204 -h 180 $DSN_BiPedAnimation;
}

DSN_RapidAnim();
