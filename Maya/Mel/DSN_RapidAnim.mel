global proc controlsUpDn()
{
	string $rig[] = `ls -sl`;
	int $rigSize = `size $rig`;
	if ($rigSize > 0)
	{
		int $identification = `getAttr {$rig[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}
	
		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
		if ($identification != 1122)
		{pickWalk -d up;}

		string $currentControl[] = `ls -sl`;
		int $identification = `getAttr {$currentControl[0] + ".identification"}`;
