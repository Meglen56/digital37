	setAttr ({$rigName[0] + "_rUpperArmCurve2C.translate"}, ($lUpperArmCurve2C_T[0] * -1), ($lUpperArmCurve2C_T[1] * -1), ($lUpperArmCurve2C_T[2] * -1));
	
