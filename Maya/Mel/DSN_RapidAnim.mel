	}
}


global proc mirror()
{
	string $rigName[] = `ls -sl`;
	//CENTER CONTROLS
	$ROOTC_R = `xform -q -ro {$rigName[0] + "_ROOTC"}`;
	setAttr {$rigName[0] + "_ROOTC.ry"} ($ROOTC_R[1] * -1);
	setAttr {$rigName[0] + "_ROOTC.rz"} ($ROOTC_R[2] * -1);
	
	$MainHipC_R = `xform -q -ro {$rigName[0] + "_MainHipC"}`;
	$MainHipC_T = `xform -q -t {$rigName[0] + "_MainHipC"}`;
	setAttr {$rigName[0] + "_MainHipC.ry"} ($MainHipC_R[1] * -1);
	setAttr {$rigName[0] + "_MainHipC.rz"} ($MainHipC_R[2] * -1);
	setAttr {$rigName[0] + "_MainHipC.tx"} ($MainHipC_T[0] * -1);

	select {$rigName[0] + "_Spine??FKC"};
	$SpineFKC = `ls -sl`;
	int $SpineNum = `size $SpineFKC`;
	int $CurrentNum = 1;
	string $sVertebrae = "_Spine";
	while ($CurrentNum <= $SpineNum)
	{
		string $currentString = $CurrentNum;
		if ($CurrentNum < 10)
		{
		   $sVertebrae = $rigName[0] + "_Spine0" + $currentString + "FKC";
		}
		else
		{
		   $sVertebrae = $rigName[0] + "_Spine" + $currentString + "FKC";
		}
			
		$SpineFKC_R = `xform -q -ro $sVertebrae`;
		$SpineFKC_T = `xform -q -t  $sVertebrae`;
		setAttr {$sVertebrae + ".ry"} ($SpineFKC_R[1] * -1);
		setAttr {$sVertebrae + ".rz"} ($SpineFKC_R[2] * -1);
		setAttr {$sVertebrae + ".tx"} ($SpineFKC_T[0] * -1);
		
		$CurrentNum = $CurrentNum + 1;
	}


	$SpineMidIKC_R = `xform -q -ro {$rigName[0] + "_SpineMidIKC"}`;
	$SpineMidIKC_T = `xform -q -t {$rigName[0] + "_SpineMidIKC"}`;
	setAttr {$rigName[0] + "_SpineMidIKC.ry"} ($SpineMidIKC_R[1] * -1);
	setAttr {$rigName[0] + "_SpineMidIKC.rz"} ($SpineMidIKC_R[2] * -1);
	setAttr {$rigName[0] + "_SpineMidIKC.tx"} ($SpineMidIKC_T[0] * -1);
	
	$SpineTopIKC_R = `xform -q -ro {$rigName[0] + "_SpineTopIKC"}`;
	$SpineTopIKC_T = `xform -q -t {$rigName[0] + "_SpineTopIKC"}`;
	setAttr {$rigName[0] + "_SpineTopIKC.ry"} ($SpineTopIKC_R[1] * -1);
	setAttr {$rigName[0] + "_SpineTopIKC.rz"} ($SpineTopIKC_R[2] * -1);
	setAttr {$rigName[0] + "_SpineTopIKC.tx"} ($SpineTopIKC_T[0] * -1);

	if (`objExists ($rigName[0] + "_spine01CurveC")`)
	{
		select {$rigName[0] + "_spine??CurveC"};
		$SpineCurveC = `ls -sl`;
		$SpineCurveNum = `size $SpineCurveC`;
		string $SpineCurves = "spine";
		int $currentNum = 1;
		while ($currentNum <= $SpineCurveNum)
		{
			string $currentString = $currentNum;
			if ($currentNum < 10)
			{
				$SpineCurves = $rigName[0] + "_spine0" + $currentString + "CurveC";
			}
			else
			{
				$SpineCurves = $rigName[0] + "_spine" + $currentString + "CurveC";
			}
			$SpineCurveC_R = `xform -q -ro $SpineCurves`;
			$SpineCurveC_T = `xform -q -t  $SpineCurves`;
			setAttr {$SpineCurves + ".ry"} ($SpineCurveC_R[1] * -1);
			setAttr {$SpineCurves + ".rz"} ($SpineCurveC_R[2] * -1);
			setAttr {$SpineCurves + ".tx"} ($SpineCurveC_T[0] * -1);
			$currentNum = $currentNum + 1;
		}
		
	}
	
	$Neck01C_R = `xform -q -ro {$rigName[0] + "_Neck01C"}`;
	$Neck01C_T = `xform -q -t {$rigName[0] + "_Neck01C"}`;
	setAttr {$rigName[0] + "_Neck01C.ry"} ($Neck01C_R[1] * -1);
	setAttr {$rigName[0] + "_Neck01C.rz"} ($Neck01C_R[2] * -1);
	
	$Neck02C_R = `xform -q -ro {$rigName[0] + "_Neck02C"}`;
	$Neck02C_T = `xform -q -t {$rigName[0] + "_Neck02C"}`;
	setAttr {$rigName[0] + "_Neck02C.ry"} ($Neck02C_R[1] * -1);
	setAttr {$rigName[0] + "_Neck02C.rz"} ($Neck02C_R[2] * -1);
	setAttr {$rigName[0] + "_Neck02C.tx"} ($Neck02C_T[0] * -1);

	if (`objExists ($rigName[0] + "_JawC")`)
	{
		$JawC_R = `xform -q -ro {$rigName[0] + "_JawC"}`;
		$JawC_T = `xform -q -t {$rigName[0] + "_JawC"}`;
		setAttr {$rigName[0] + "_JawC.ry"} ($JawC_R[1] * -1);
		setAttr {$rigName[0] + "_JawC.rz"} ($JawC_R[2] * -1);
		setAttr {$rigName[0] + "_JawC.tx"} ($JawC_T[0] * -1);
	}
	
	$HeadC_R = `xform -q -ro {$rigName[0] + "_HeadC"}`;
	$HeadC_T = `xform -q -t {$rigName[0] + "_HeadC"}`;
	setAttr {$rigName[0] + "_HeadC.ry"} ($HeadC_R[1] * -1);
	setAttr {$rigName[0] + "_HeadC.rz"} ($HeadC_R[2] * -1);
	setAttr {$rigName[0] + "_HeadC.tx"} ($HeadC_T[0] * -1);
	

	if (`objExists ($rigName[0] + "_EyeAimC")`)
	{
		$EyeAimC_T = `xform -q -t {$rigName[0] + "_EyeAimC"}`;
		setAttr {$rigName[0] + "_EyeAimC.tx"} ($EyeAimC_T[0] * -1);
	}

	//EYES	
	if (`objExists ($rigName[0] + "_lEyeAimC")`)
	{
		$lEyeAimC_T = `xform -q -t {$rigName[0] + "_lEyeAimC"}`;
		$rEyeAimC_T = `xform -q -t {$rigName[0] + "_rEyeAimC"}`;
		setAttr ({$rigName[0] + "_rEyeAimC.translate"}, ($lEyeAimC_T[0] * -1), $lEyeAimC_T[1], $lEyeAimC_T[2]);
		setAttr ({$rigName[0] + "_lEyeAimC.translate"}, ($rEyeAimC_T[0] * -1), $rEyeAimC_T[1], $rEyeAimC_T[2]);
	
		if (`objExists ($rigName[0] + "_geoSkinG")`)
		{
		$lEyeTopOC = `getAttr {$rigName[0] + "_lEyeAimC.TopOpenClose"}`;
		$lEyeBtmOC = `getAttr {$rigName[0] + "_lEyeAimC.BtmOpenClose"}`;
		$lEyeTopExp = `getAttr {$rigName[0] + "_lEyeAimC.TopExpress"}`;
		$lEyeBtmExp = `getAttr {$rigName[0] + "_lEyeAimC.BtmExpress"}`;
		$lEyeFollow = `getAttr {$rigName[0] + "_lEyeAimC.LidsFollow"}`;
    	
		$rEyeTopOC = `getAttr {$rigName[0] + "_rEyeAimC.TopOpenClose"}`;
		$rEyeBtmOC = `getAttr {$rigName[0] + "_rEyeAimC.BtmOpenClose"}`;
		$rEyeTopExp = `getAttr {$rigName[0] + "_rEyeAimC.TopExpress"}`;
		$rEyeBtmExp = `getAttr {$rigName[0] + "_rEyeAimC.BtmExpress"}`;
		$rEyeFollow = `getAttr {$rigName[0] + "_rEyeAimC.LidsFollow"}`;
    	
		setAttr {$rigName[0] + "_rEyeAimC.TopOpenClose"} $lEyeTopOC;
		setAttr {$rigName[0] + "_rEyeAimC.BtmOpenClose"} $lEyeBtmOC;
		setAttr {$rigName[0] + "_rEyeAimC.TopExpress"} $lEyeTopExp;
		setAttr {$rigName[0] + "_rEyeAimC.BtmExpress"} $lEyeBtmExp;
		setAttr {$rigName[0] + "_rEyeAimC.LidsFollow"} $lEyeFollow;
		
		setAttr {$rigName[0] + "_lEyeAimC.TopOpenClose"} $rEyeTopOC;
		setAttr {$rigName[0] + "_lEyeAimC.BtmOpenClose"} $rEyeBtmOC;
		setAttr {$rigName[0] + "_lEyeAimC.TopExpress"} $rEyeTopExp;
		setAttr {$rigName[0] + "_lEyeAimC.BtmExpress"} $rEyeBtmExp;
		setAttr {$rigName[0] + "_lEyeAimC.LidsFollow"} $rEyeFollow;
		}
	}
	
	//ARMS////////////////////////////////////////////////////////////////////////
	$lClavicleC_R = `xform -q -ro {$rigName[0] + "_lClavicleC"}`;
	$rClavicleC_R = `xform -q -ro {$rigName[0] + "_rClavicleC"}`;
	setAttr ({$rigName[0] + "_lClavicleC.rotate"}, $rClavicleC_R[0], $rClavicleC_R[1], $rClavicleC_R[2]);
	setAttr ({$rigName[0] + "_rClavicleC.rotate"}, $lClavicleC_R[0], $lClavicleC_R[1], $lClavicleC_R[2]);
	
	//FK SHOULDERS
	$lShoulderFKC_R = `xform -q -ro {$rigName[0] + "_lShoulderFKC"}`;
	$rShoulderFKC_R = `xform -q -ro {$rigName[0] + "_rShoulderFKC"}`;
	$lShoulderFKC_SX = `getAttr {$rigName[0] + "_lShoulderFKC.sx"}`;	
	$rShoulderFKC_SX = `getAttr {$rigName[0] + "_rShoulderFKC.sx"}`;	
	$lShoulderFKC_Orient = `getAttr {$rigName[0] + "_lShoulderFKC.ShoulderOrient"}`;	
	$rShoulderFKC_Orient = `getAttr {$rigName[0] + "_rShoulderFKC.ShoulderOrient"}`;	
	setAttr ({$rigName[0] + "_lShoulderFKC.rotate"}, $rShoulderFKC_R[0], $rShoulderFKC_R[1], $rShoulderFKC_R[2]);
	setAttr {$rigName[0] + "_lShoulderFKC.sx"} $rShoulderFKC_SX;
	setAttr {$rigName[0] + "_lShoulderFKC.ShoulderOrient"} $rShoulderFKC_Orient;

	setAttr ({$rigName[0] + "_rShoulderFKC.rotate"}, $lShoulderFKC_R[0], $lShoulderFKC_R[1], $lShoulderFKC_R[2]);	
	setAttr {$rigName[0] + "_rShoulderFKC.sx"} $lShoulderFKC_SX;
	setAttr {$rigName[0] + "_rShoulderFKC.ShoulderOrient"} $lShoulderFKC_Orient;
	
	if (`objExists ($rigName[0] + "_rShoulderFKC.ClavicleInfluence")`)
	{
		$lShoulderFKC_ClavInf = `getAttr {$rigName[0] + "_lShoulderFKC.ClavicleInfluence"}`;	
		$rShoulderFKC_ClavInf = `getAttr {$rigName[0] + "_rShoulderFKC.ClavicleInfluence"}`;	
		setAttr {$rigName[0] + "_rShoulderFKC.ClavicleInfluence"} $lShoulderFKC_ClavInf;
		setAttr {$rigName[0] + "_lShoulderFKC.ClavicleInfluence"} $rShoulderFKC_ClavInf;
	}
	
	//FK ELBOWS
	$lElbowFKC_R = `getAttr {$rigName[0] + "_lElbowFKC.ry"}`;
	$rElbowFKC_R = `getAttr {$rigName[0] + "_rElbowFKC.ry"}`;
	setAttr {$rigName[0] + "_lElbowFKC.ry"} $rElbowFKC_R;
	setAttr {$rigName[0] + "_rElbowFKC.ry"} $lElbowFKC_R;
	//FK WRISTS
	$lWristFKC_R = `xform -q -ro {$rigName[0] + "_lWristFKC"}`;
	$rWristFKC_R = `xform -q -ro {$rigName[0] + "_rWristFKC"}`;
	$lWristFKC_SX = `getAttr {$rigName[0] + "_lWristFKC.sx"}`;	
	$rWristFKC_SX = `getAttr {$rigName[0] + "_rWristFKC.sx"}`;	
	setAttr ({$rigName[0] + "_lWristFKC.rotate"}, $rWristFKC_R[0], $rWristFKC_R[1], $rWristFKC_R[2]);
	setAttr {$rigName[0] + "_lWristFKC.sx"} $rWristFKC_SX;
	setAttr ({$rigName[0] + "_rWristFKC.rotate"}, $lWristFKC_R[0], $lWristFKC_R[1], $lWristFKC_R[2]);	
	setAttr {$rigName[0] + "_rWristFKC.sx"} $lWristFKC_SX;
	
	//IK ELBOWS
	$lElbowIKC_T = `xform -q -t {$rigName[0] + "_lElbowIKC"}`;
	$rElbowIKC_T = `xform -q -t {$rigName[0] + "_rElbowIKC"}`;	
	setAttr ({$rigName[0] + "_rElbowIKC.translate"}, ($lElbowIKC_T[0] * -1), $lElbowIKC_T[1], $lElbowIKC_T[2]);
	setAttr ({$rigName[0] + "_lElbowIKC.translate"}, ($rElbowIKC_T[0] * -1), $rElbowIKC_T[1], $rElbowIKC_T[2]);

	//IK WRISTS
	$lWristIKC_R = `xform -q -ro {$rigName[0] + "_lWristIKC"}`;
	$lWristIKC_T = `xform -q -t {$rigName[0] + "_lWristIKC"}`;
	$lWristIKC_SX = `getAttr {$rigName[0] + "_lWristIKC.sx"}`;
	$lWristIKC_Stretch = `getAttr {$rigName[0] + "_lWristIKC.Stretch"}`;	
	$lWristIKC_ElbowLock = `getAttr {$rigName[0] + "_lWristIKC.ElbowLock"}`;
	$lWristIKC_Hips = `getAttr {$rigName[0] + "_lWristIKC.Hips"}`;	
	$lWristIKC_Chest = `getAttr {$rigName[0] + "_lWristIKC.Chest"}`;	
	$lWristIKC_Head = `getAttr {$rigName[0] + "_lWristIKC.Head"}`;
	
	$rWristIKC_R = `xform -q -ro {$rigName[0] + "_rWristIKC"}`;
	$rWristIKC_T = `xform -q -t {$rigName[0] + "_rWristIKC"}`;
	$rWristIKC_SX = `getAttr {$rigName[0] + "_rWristIKC.sx"}`;
	$rWristIKC_Stretch = `getAttr {$rigName[0] + "_rWristIKC.Stretch"}`;	
	$rWristIKC_ElbowLock = `getAttr {$rigName[0] + "_rWristIKC.ElbowLock"}`;
	$rWristIKC_Hips = `getAttr {$rigName[0] + "_rWristIKC.Hips"}`;	
	$rWristIKC_Chest = `getAttr {$rigName[0] + "_rWristIKC.Chest"}`;	
	$rWristIKC_Head = `getAttr {$rigName[0] + "_rWristIKC.Head"}`;
	
	setAttr ({$rigName[0] + "_rWristIKC.translate"}, ($lWristIKC_T[0] * -1), ($lWristIKC_T[1] * -1), ($lWristIKC_T[2] * -1));
	setAttr ({$rigName[0] + "_rWristIKC.rotate"}, $lWristIKC_R[0], $lWristIKC_R[1], $lWristIKC_R[2]);	
	setAttr {$rigName[0] + "_rWristIKC.sx"} $lWristIKC_SX;
	setAttr {$rigName[0] + "_rWristIKC.Stretch"} $lWristIKC_Stretch;
	setAttr {$rigName[0] + "_rWristIKC.ElbowLock"} $lWristIKC_ElbowLock;
	setAttr {$rigName[0] + "_rWristIKC.Hips"} $lWristIKC_Hips;
	setAttr {$rigName[0] + "_rWristIKC.Chest"} $lWristIKC_Chest;
	setAttr {$rigName[0] + "_rWristIKC.Head"} $lWristIKC_Head;
	
	setAttr ({$rigName[0] + "_lWristIKC.translate"}, ($rWristIKC_T[0] * -1), ($rWristIKC_T[1] * -1), ($rWristIKC_T[2] * -1));
	setAttr ({$rigName[0] + "_lWristIKC.rotate"}, $rWristIKC_R[0], $rWristIKC_R[1], $rWristIKC_R[2]);	
	setAttr {$rigName[0] + "_lWristIKC.sx"} $rWristIKC_SX;
	setAttr {$rigName[0] + "_lWristIKC.Stretch"} $rWristIKC_Stretch;
	setAttr {$rigName[0] + "_lWristIKC.ElbowLock"} $rWristIKC_ElbowLock;
	setAttr {$rigName[0] + "_lWristIKC.Hips"} $rWristIKC_Hips;
	setAttr {$rigName[0] + "_lWristIKC.Chest"} $rWristIKC_Chest;
	setAttr {$rigName[0] + "_lWristIKC.Head"} $rWristIKC_Head;
	//PALM
	$lPalmC_R = `xform -q -ro {$rigName[0] + "_lPalmC"}`;
	$rPalmC_R = `xform -q -ro {$rigName[0] + "_rPalmC"}`;
	setAttr ({$rigName[0] + "_lPalmC.rotate"}, $rPalmC_R[0], $rPalmC_R[1], $rPalmC_R[2]);
	setAttr ({$rigName[0] + "_rPalmC.rotate"}, $lPalmC_R[0], $lPalmC_R[1], $lPalmC_R[2]);
		
	
	//ARM SWITCH
	$lArmSwitch = `getAttr {$rigName[0] + "_lArmSwitchC.SwitchIkFk"}`;
	$rArmSwitch = `getAttr {$rigName[0] + "_rArmSwitchC.SwitchIkFk"}`;
	$lArmVolume = `getAttr {$rigName[0] + "_lArmSwitchC.AutoVolume"}`;
	$rArmVolume = `getAttr {$rigName[0] + "_rArmSwitchC.AutoVolume"}`;	
	setAttr {$rigName[0] + "_lArmSwitchC.SwitchIkFk"} $rArmSwitch;
	setAttr {$rigName[0] + "_rArmSwitchC.SwitchIkFk"} $lArmSwitch;
	setAttr {$rigName[0] + "_lArmSwitchC.AutoVolume"} $rArmSwitch;
	setAttr {$rigName[0] + "_rArmSwitchC.AutoVolume"} $lArmVolume;
	
	//LEGS////////////////////////////////////////////////////////////////////////
	//FK HIPS
	$lHipFKC_R = `xform -q -ro {$rigName[0] + "_lHipFKC"}`;
	$rHipFKC_R = `xform -q -ro {$rigName[0] + "_rHipFKC"}`;
	$lHipFKC_SX = `getAttr {$rigName[0] + "_lHipFKC.sx"}`;	
	$rHipFKC_SX = `getAttr {$rigName[0] + "_rHipFKC.sx"}`;	
	$lHipFKC_Orient = `getAttr {$rigName[0] + "_lHipFKC.HipOrient"}`;	
	$rHipFKC_Orient = `getAttr {$rigName[0] + "_rHipFKC.HipOrient"}`;	
	setAttr ({$rigName[0] + "_lHipFKC.rotate"}, $rHipFKC_R[0], $rHipFKC_R[1], $rHipFKC_R[2]);
	setAttr {$rigName[0] + "_lHipFKC.sx"} $rHipFKC_SX;
	setAttr {$rigName[0] + "_lHipFKC.HipOrient"} $rHipFKC_Orient;

	setAttr ({$rigName[0] + "_rHipFKC.rotate"}, $lHipFKC_R[0], $lHipFKC_R[1], $lHipFKC_R[2]);	
	setAttr {$rigName[0] + "_rHipFKC.sx"} $lHipFKC_SX;
	setAttr {$rigName[0] + "_rHipFKC.HipOrient"} $lHipFKC_Orient;
	//FK KNEES
	$lKneeFKC_R = `getAttr {$rigName[0] + "_lKneeFKC.rz"}`;
	$rKneeFKC_R = `getAttr {$rigName[0] + "_rKneeFKC.rz"}`;
	setAttr {$rigName[0] + "_lKneeFKC.rz"} $rKneeFKC_R;
	setAttr {$rigName[0] + "_rKneeFKC.rz"} $lKneeFKC_R;
	//FK ANKLES
	$lAnkleFKC_R = `xform -q -ro {$rigName[0] + "_lAnkleFKC"}`;
	$rAnkleFKC_R = `xform -q -ro {$rigName[0] + "_rAnkleFKC"}`;
	setAttr ({$rigName[0] + "_lAnkleFKC.rotate"}, $rAnkleFKC_R[0], $rAnkleFKC_R[1], $rAnkleFKC_R[2]);
	setAttr ({$rigName[0] + "_rAnkleFKC.rotate"}, $lAnkleFKC_R[0], $lAnkleFKC_R[1], $lAnkleFKC_R[2]);	
	//FK BALLS
	$lBallFKC_R = `xform -q -ro {$rigName[0] + "_lBallFKC"}`;
	$rBallFKC_R = `xform -q -ro {$rigName[0] + "_rBallFKC"}`;
	setAttr ({$rigName[0] + "_lBallFKC.rotate"}, $rBallFKC_R[0], $rBallFKC_R[1], $rBallFKC_R[2]);
	setAttr ({$rigName[0] + "_rBallFKC.rotate"}, $lBallFKC_R[0], $lBallFKC_R[1], $lBallFKC_R[2]);
	
	//IK KNEES
	$lKneeIKC_T = `xform -q -t {$rigName[0] + "_lKneeIKC"}`;
	$rKneeIKC_T = `xform -q -t {$rigName[0] + "_rKneeIKC"}`;
	setAttr ({$rigName[0] + "_rKneeIKC.translate"}, ($lKneeIKC_T[0] * -1), $lKneeIKC_T[1], $lKneeIKC_T[2]);
	setAttr ({$rigName[0] + "_lKneeIKC.translate"}, ($rKneeIKC_T[0] * -1), $rKneeIKC_T[1], $rKneeIKC_T[2]);

